{% extends "topologies.jinja2" %}

{% block contentTopologies %}

<div id="aum" class="container-fluid">
  <div class="row-fluid">
    <div class="span12">
      <h3 align="center">Links Agreements</h3>
    </div>
  </div>
  <div class="row-fluid" style="text-align: center;">
      <div class="span12">
        <table border="0" style="width: 100%;">
          <tr>
            <td><h4>Agreement</h4></td>
            <td><h4>Description</h4></td>
          </tr>
          {% for agreement in agreements %}
            <tr>
              <td style="width: 30%;"><p> {{ agreement.agreement }} </p></td>
              <td style="text-align: left; width: 70%;"><p> {{ agreement.description }} </p></td>
            </tr>
          {% endfor %}
        </table>
      </div>
  </div>
</div>

{% endblock contentTopologies %}